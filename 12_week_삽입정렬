
#include <stdio.h>
#include <time.h>
#include <stdlib.h>
#define MAX_SIZE 100
void insertion_sort(int list[], int n)
{
    int i,j,key;
    for(i=0;i<n;i++){
        key=list[i];
        for(j=i-1; j>=0&&list[j]>key;j--)
            list[j+1]=list[j];
        list[j+1]=key;
    }
}
int main()
{
    int i,j;
    int n = MAX_SIZE;
    int list[n];
    srand(time(NULL));
    for (i = 0; i < n; i++) {
		list[i] = rand() % 300;
		for (j = 0; j < i; j++)
			if (list[i] == list[j]) {
				i--;
				break;
			}
	}
	printf("정렬 전 : \n ");
    for (i=0;i<n;i++)
        printf("%d ",list[i]);
    printf("\n");  
	printf("정렬 후 : \n ");
    insertion_sort(list,n);
    for (i=0;i<n;i++)
        printf("%d ",list[i]);
    printf("\n");    
    return 0;
}
