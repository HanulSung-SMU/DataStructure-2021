#include <stdio.h>
#include <memory.h>

  

typedef struct TreeNode {
    char data;
    struct TreeNode *left, *right;
} TreeNode;
TreeNode n1={'a', NULL, NULL};
TreeNode n2={'b', NULL, NULL};
TreeNode n3={'+', &n1, &n2};
TreeNode *root = &n3;

int inorder(TreeNode *root){
    if (root){
        inorder(root->left);
        printf("%c",root->data);
        inorder(root->right);
        
    }
}

int preorder(TreeNode *root){
    if(root){
        printf("%c",root->data);
        
        preorder(root->left);
        preorder(root->right);
        
    }
}

int postorder(TreeNode *root){
    if(root){
        postorder(root->left);
        postorder(root->right);
        printf("%c",root->data);
        
    }
}

int main(void)
{
    
    printf("중위 순회=");
    inorder(root);
    printf("\n");
    
    printf("전위 순휘=");
    preorder(root);
    printf("\n");
    
    printf("후위 순회=");
    postorder(root);
    printf("\n");
    
    
    return 0;
}
